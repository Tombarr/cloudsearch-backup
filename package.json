{
    "name": "cloudsearch-backup",
    "description": "Backup and restore CloudSearch to S3",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">=20.0.0"
    },
    "author": "Tom Barrasso",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Tombarr/cloudsearch-backup/issues"
    },
    "homepage": "https://github.com/Tombarr/cloudsearch-backup#readme",
    "devDependencies": {
        "jest": "^29.2.1"
    },
    "keywords": [
        "nodejs",
        "js",
        "cloudsearch",
        "serverless",
        "backup",
        "restore",
        "lambda",
        "s3",
        "aws"
    ],
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/__tests__/**/*.mjs?(x)",
            "**/?(*.)+(spec|test).mjs?(x)"
        ],
        "moduleFileExtensions": [
            "mjs",
            "js"
        ]
    }
}
